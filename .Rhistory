}
}
print(success/100)
#| roll 1x, roll is 2 given it is even
success <- 0
even_rolls <- 0
for(i in 1:100){
first_roll <- sample(x = 6, size = 1, replace = T)
if(first_roll %% 2 == 0){
even_rolls <- even_rolls + 1
if(first_roll == 2){
success <- success + 1
}
else{
success <- success + 0
}
}
else{
even_rolls <- even_rolls +0
}
}
print(success/even_rolls)
#| label: roll 1x, first roll is even given that it is greater than 1?
success <- 0
roles_greaterthan_1 <- 0
for(i in 1:100){
first_roll <- sample(x = 6, size = 1, replace = T)
if(first_roll > 1){
roles_greaterthan_1 <- roles_greaterthan_1 + 1
if(first_roll %% 2 == 0){
success <- success + 1
}
else{
success <- success + 0
}
}
else{
roles_greaterthan_1 <- roles_greaterthan_1 +0
}
}
print(success/roles_greaterthan_1)
