support0_10 <- c(seq(0,10))
XPDF <- function(x,p){
((1-p)^x)*p
}
barplot(XPDF(support0_10,0.5), ylab = "probability density", xlab = "x", main = "PDF for X")
YPDF <- function(y,l){
l*(exp(1)^(-l*y))
}
plot(YPDF(support0_10,0.5), ylab = "probability density", xlab = "y", main = "PDF for Y")
XPDF(5,0.5)
