True_XVar <- function(Xset){
Xrunning_sum <- 0
Xmean <- mean(Xset)
for (i in 1:length(Xset)) {
Xrunning_sum <- Xrunning_sum + ((Xset[i] - Xmean)^2)
}
Xvar_result <- Xrunning_sum/length(Xset)
print(Xvar_result)
}
