success <- 0
for(i in 1:100){
first_roll <- sample(x = 6, size = 1, replace = T)
second_roll <- sample(x = 6, size = 1, replace = T)
if(first_roll + second_roll == 3){
success <- success + 1
}
else{
success <- success + 0
}
}
print(success/100)
#| label: roll 2x, observing same value both times
